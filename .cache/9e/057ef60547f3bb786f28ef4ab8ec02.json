rtist(artist) )\r\n\t\t}\r\n\t})\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\nfunction getAppContext() {\r\n\treturn document.querySelector('#app')\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"22dbd3b84a5437d6ff77861975a7ab39","cacheData":{"env":{}}}